{
    "name": "grandine",
    "description": "Fast PoS and Sharding client supporting Ethereum 2.0 networks",
    "usage": "grandine [OPTIONS] [COMMAND]",
    "subcommands": [
        {
            "name": "grandine export",
            "description": "Export blocks and state to ssz files within slot range for debugging",
            "usage": "grandine export [OPTIONS] --from <SLOT> --to <SLOT>",
            "options": [
                {
                    "option": "--from",
                    "shortcut": "-f",
                    "description": "First slot to export (inclusive)",
                    "value": "<SLOT>"
                },
                {
                    "option": "--help",
                    "shortcut": "-h",
                    "description": "Print help"
                },
                {
                    "option": "--output-dir",
                    "shortcut": "-o",
                    "description": "Output directory",
                    "value": "<OUTPUT_DIR>",
                    "default": "current directory"
                },
                {
                    "option": "--to",
                    "shortcut": "-t",
                    "description": "Last slot to export (inclusive)",
                    "value": "<SLOT>"
                }
            ],
            "subcommands": []
        },
        {
            "name": "grandine interchange",
            "description": "Import/export slashing protection interchange file (example: grandine interchange import file.json)",
            "usage": "grandine interchange <COMMAND>",
            "subcommands": [
                {
                    "name": "grandine interchange export",
                    "description": "Export slashing protection interchange file",
                    "usage": "grandine interchange export <FILE_PATH>",
                    "subcommands": [
                    ],
                    "options": [
                        {
                            "option": "--help",
                            "shortcut": "-h",
                            "description": "Print help"
                        }
                    ]
                },
                {
                    "name": "grandine interchange import",
                    "description": "Import slashing protection interchange file",
                    "usage": "grandine interchange import <FILE_PATH>",
                    "subcommands": [],
                    "options": [
                        {
                            "option": "--help",
                            "shortcut": "-h",
                            "description": "Print help"
                        }
                    ]
                }
            ],
            "options": [
                {
                    "option": "--help",
                    "shortcut": "-h",
                    "description": "Print help"
                }
            ]
        },
        {
            "name": "grandine replay",
            "description": "Replay blocks within slot range",
            "usage": "grandine replay [OPTIONS] --from <SLOT> --to <SLOT>",
            "subcommands": [],
            "options": [
                {
                    "option": "--from",
                    "shortcut": "-f",
                    "description": "Replay start slot (inclusive)",
                    "value": "<SLOT>"
                },
                {
                    "option": "--help",
                    "shortcut": "-h",
                    "description": "Print help"
                },
                {
                    "option": "--input-dir",
                    "shortcut": "-i",
                    "description": "Input directory",
                    "value": "<INPUT_DIR>",
                    "default": "current directory"
                },
                {
                    "option": "--to",
                    "shortcut": "-t",
                    "description": "Replay end slot (inclusive)",
                    "value": "<SLOT>"
                }
            ]
        }
    ],
