{
  "name": "ps",
  "version": "4.0.4",
  "usage": [
    "ps [options]"
  ],
  "subcommands": [],
  "options": [
    {
      "option": "-V",
      "shortcut": "",
      "description": "prints version",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-h",
      "shortcut": "",
      "description": "prints available commands usage",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-x",
      "shortcut": "",
      "description": "bughunt mode - implies tracemode",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-d",
      "shortcut": "",
      "description": "delay for update refresh rate in seconds. default is 1",
      "value": "seconds",
      "default": "1",
      "section": "Options"
    },
    {
      "option": "-v",
      "shortcut": "",
      "description": "view mode (0 = KB/s, 1 = total KB, 2 = total B, 3 = total MB, 4 = MB/s, 5 = GB/s). default is 0",
      "value": "mode",
      "default": "0",
      "section": "Options"
    },
    {
      "option": "-c",
      "shortcut": "",
      "description": "number of updates. default is 0 (unlimited)",
      "value": "count",
      "default": "0",
      "section": "Options"
    },
    {
      "option": "-t",
      "shortcut": "",
      "description": "tracemode",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-p",
      "shortcut": "",
      "description": "sniff in promiscuous mode (not recommended)",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-s",
      "shortcut": "",
      "description": "sort output by sent column",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-l",
      "shortcut": "",
      "description": "display command line",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-a",
      "shortcut": "",
      "description": "monitor all devices, even loopback/stopped ones",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-C",
      "shortcut": "",
      "description": "capture TCP and UDP",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-b",
      "shortcut": "",
      "description": "Display the program basename",
      "value": null,
      "default": null,
      "section": "Options"
    },
    {
      "option": "-g",
      "shortcut": "",
      "description": "garbage collection period in number of refresh. default is 50",
      "value": "period",
      "default": "50",
      "section": "Options"
    },
    {
      "option": "-P",
      "shortcut": "",
      "description": "Show only processes with the specified pid(s)",
      "value": "pid",
      "default": null,
      "section": "Options"
    },
    {
      "option": "-f",
      "shortcut": "",
      "description": "EXPERIMENTAL: specify string pcap filter (like tcpdump). This may be removed or changed in a future version",
      "value": "filter",
      "default": null,
      "section": "Options"
    },
    {
      "option": "device(s)",
      "shortcut": "",
      "description": "to monitor. default is all interfaces up and running excluding loopback",
      "value": null,
      "default": "all interfaces up and running excluding loopback",
      "section": "Options"
    }
  ],
  "interactive_controls": [
    {
      "key": "q",
      "description": "quit"
    },
    {
      "key": "s",
      "description": "sort by SENT traffic"
    },
    {
      "key": "r",
      "description": "sort by RECEIVED traffic"
    },
    {
      "key": "l",
      "description": "display command line"
    },
    {
      "key": "b",
      "description": "display the program basename"
    },
    {
      "key": "m",
      "description": "switch between total (KB, B, MB) and throughput (KB/s, MB/s, GB/s) mode"
    }
  ]
