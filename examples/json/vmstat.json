{
    "name": "vmstat",
    "description": "For more details see vmstat(8).",
    "usage": "vmstat [options] [delay [count]]",
    "options": [
        {
            "option": "--active",
            "shortcut": "-a",
            "description": "active/inactive memory"
        },
        {
            "option": "--disk",
            "shortcut": "-d",
            "description": "disk statistics"
        },
        {
            "option": "--disk-sum",
            "shortcut": "-D",
            "description": "summarize disk statistics"
        },
        {
            "option": "--forks",
            "shortcut": "-f",
            "description": "number of forks since boot"
        },
        {
            "option": "--help",
            "shortcut": "-h",
            "description": "display this help and exit"
        },
        {
            "option": "--no-first",
            "shortcut": "-y",
            "description": "skips first line of output"
        },
        {
            "option": "--one-header",
            "shortcut": "-n",
            "description": "do not redisplay header"
        },
        {
            "option": "--partition",
            "shortcut": "-p",
            "description": "partition specific statistics",
            "value": "<dev>"
        },
        {
            "option": "--slabs",
            "shortcut": "-m",
            "description": "slabinfo"
        },
        {
            "option": "--stats",
            "shortcut": "-s",
            "description": "event counter statistics"
        },
        {
            "option": "--timestamp",
            "shortcut": "-t",
            "description": "show timestamp"
        },
        {
            "option": "--unit",
            "shortcut": "-S",
            "description": "define display unit",
            "value": "<char>"
        },
        {
            "option": "--version",
            "shortcut": "-V",
            "description": "output version information and exit"
        },
        {
            "option": "--wide",
            "shortcut": "-w",
            "description": "wide output"
        }
    ],
    "subcommands": [],
    "version": "4.0.4"
}
